<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>TestFx.Farada</id>
    <version>$version$</version>
    <authors>Matthias Koch</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>TestFx + Farada = &lt;3</description>
    <iconUrl>http://matkoch.github.io/TestFx/TestFx.ico</iconUrl>
    <projectUrl>https://github.com/matkoch/TestFx</projectUrl>
    <licenseUrl>https://raw.github.com/matkoch/TestFx/master/LICENSE</licenseUrl>
    <tags>testfx farada</tags>
    <dependencies>
      <dependency id="TestFx.Core" version="$version$" />
      <dependency id="TestFx.SpecK" version="$version$" />
      <dependency id="Farada.TestDataGeneration" version="[0.1.6,0.2.0)" />
    </dependencies>
  </metadata>
  <files>
    <file src="TestFx.Farada\bin\$configuration$\TestFx.Farada.*" target="lib\net40" />
    <file src="TestFx.Farada\**\*.cs" exclude="obj\**\*" target="src\TestFx.Farada" />
    <file src="TestFx.Farada\NuGet\content\**\*" target="content" />
  </files>
</package>