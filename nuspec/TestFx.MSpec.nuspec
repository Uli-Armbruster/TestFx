<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>TestFx.MSpec</id>
    <version>$version$</version>
    <authors>Matthias Koch</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>TestAdapter for Machine.Specifications.</description>
    <iconUrl>http://matkoch.github.io/TestFx/TestFx.ico</iconUrl>
    <projectUrl>https://github.com/matkoch/TestFx</projectUrl>
    <licenseUrl>https://raw.github.com/matkoch/TestFx/master/LICENSE</licenseUrl>
    <tags>testfx specifications tdd bdd unit-testing mspec</tags>
    <dependencies>
      <dependency id="TestFx.Core" version="$version$" />
    </dependencies>
  </metadata>
  <files>
    <file src="TestFx.MSpec\bin\$configuration$\TestFx.MSpec.*" target="lib\net40" />
    <file src="TestFx.MSpec\**\*.cs" exclude="obj\**\*" target="src\TestFx.MSpec" />
    <file src="TestFx.MSpec\NuGet\tools\**\*" target="tools" />
  </files>
</package>