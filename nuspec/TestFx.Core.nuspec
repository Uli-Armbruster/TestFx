<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>TestFx.Core</id>
    <version>$version$</version>
    <authors>Matthias Koch</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>TestFx core components.</description>
    <iconUrl>http://matkoch.github.io/TestFx/TestFx.ico</iconUrl>
    <projectUrl>https://github.com/matkoch/TestFx</projectUrl>
    <licenseUrl>https://raw.github.com/matkoch/TestFx/master/LICENSE</licenseUrl>
    <tags>testfx</tags>
  </metadata>
  <files>
    <file src="TestFx.Core\bin\$configuration$\*" target="lib\net40" />
    <file src="TestFx.Core\**\*.cs" exclude="obj\**\*" target="src\TestFx.Core" />
    <file src="TestFx.Core\NuGet\content\**\*" target="content" />
    <file src="TestFx.Console\bin\$configuration$\TestFx.exe" target="tools" />
  </files>
</package>