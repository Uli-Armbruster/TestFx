<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>TestFx.Core</id>
    <version>$version$</version>
    <authors>Matthias Koch</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>TestFx core components.</description>
    <iconUrl>http://matkoch.github.io/TestFx/TestFx.ico</iconUrl>
    <projectUrl>https://github.com/matkoch/TestFx</projectUrl>
    <licenseUrl>https://raw.github.com/matkoch/TestFx/master/LICENSE</licenseUrl>
    <tags>testfx</tags>
  </metadata>
  <files>
    <file target="lib\net40" src="TestFx.Core\bin\$configuration$\*" />
    <file target="src\TestFx.Core" src="TestFx.Core\**\*.cs" exclude="obj\**\*" />
    <file target="content" src="TestFx.Core\NuGet\content\**\*" />
    <file target="tools" src="TestFx.Console\bin\$configuration$\TestFx.exe" />
    <file target="build\net40" src="TestFx.VisualStudio.TestAdapter\bin\$configuration$\TestFx.VisualStudio.*" />
    <file target="build\net40" src="TestFx.Core\NuGet\build\**\*" />
  </files>
</package>