<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>TestFx.Specifications</id>
    <version>$version$</version>
    <authors>Matthias Koch</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>TestFx for Specifications.</description>
    <iconUrl>https://raw.github.com/matkoch/TestFx/master/TestFx.ico</iconUrl>
    <projectUrl>https://github.com/matkoch/TestFx</projectUrl>
    <licenseUrl>https://raw.github.com/matkoch/TestFx/master/LICENSE</licenseUrl>
    <tags>testfx specifications tdd bdd unit-testing</tags>
    <dependencies>
      <dependency id="TestFx" version="$version$" />
    </dependencies>
  </metadata>
  <files>
    <file src="TestFx.Specifications\bin\$configuration$\TestFx.Specifications.*" target="lib\net35" />
    <file src="TestFx.Specifications\**\*.cs" exclude="obj\**\*" target="src\TestFx.Specifications" />
    <file src="TestFx.Specifications\NuGet\*" target="tools" />
  </files>
</package>